<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.boilerplate.java.entities.UserMonthlyScoreEntity"
		table="Aks_UserMonthlyScore">
		<id name="id" column="Id">
			<generator class="identity" />
		</id>
		<property name="year" column="Year" />
		<property name="userId" column="UserId" />
		<property name="month" column="Month" />
		<property name="monthlyObtainedScore" column="ObtainedScore" />
		<property name="monthlyReferScore" column="ReferScore" />
		<property name="monthlyRank" column="MonthlyRank" />
	</class>
</hibernate-mapping>