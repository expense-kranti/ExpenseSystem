<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.boilerplate.java.entities.BankData" table="BankData">
		<!-- <composite-id> <key-property name="accountNumber" column="AccountNumber" 
			/> <key-property name="bankName" column="BankName" /> </composite-id> -->
		<id name="id" column="Id">
			<generator class="identity"></generator>
		</id>
		<property name="firstName" column="FirstName" />
		<property name="lastName" column="LastName" />
		<property name="middleName" column="MiddleName" />
		<property name="accountNumber" column="AccountNumber" />
		<property name="bankName" column="BankName" />
		<property name="product" column="Product" />
		<property name="dateOpened" column="Start_Date" />
		<property name="status" column="AccountStatus"/>
			
		<property name="lastPaymentDate" column="Last_Pay_Date" />
		<property name="sanctionedAmount" column="Credit_Limit" />
		<property name="dateOfBirth" column="DOB" />
		<property name="panNumber" column="Pan" />
		<property name="aadhar" column="Aadhaar" />
		<property name="passport" column="Passport" />
		<property name="drivingLicense" column="DrivingLicense" />
		<property name="voterId" column="VoterId" />
		<property name="dataSource" column="DataSource" />
		
		<property name="addressLine1" column="Address_1" />
		<property name="addressLine2" column="Address_2" />
		<property name="addressLine3" column="Address_3" />
		<property name="addressLine4" column="Address_4" />
		<property name="city" column="City" />
		<property name="state" column="State" />
		<property name="pinCode" column="Pincode" />
		
		
	</class>
</hibernate-mapping>