<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.boilerplate.java.entities.ElectronicContact"
		table="Aks_ElectronicContact">
		<id name="id" column="Id"></id>
		<property name="tradelineId" column="TradelineId" />
		<property name="email" column="Email" />
		<property name="telephoneNumber" column="TelephoneNumber" />
		<property name="mobileNumber" column="MobileNumber" />

	</class>
</hibernate-mapping>